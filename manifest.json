{
    "manifest_version": 3,
    "name": "Video Downloader",
    "version": "1.0",
    "description": "Adiciona botões para baixar vídeos da página.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icons/icon.png"
    }
  }
  